<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
<controls>

	<control>
		<description>group element</description>
		<type>group</type>
		<animation effect="fade" time="200">WindowOpen</animation>
		<animation effect="fade" time="200">WindowClose</animation>
		<control>
			<description>composite control consisting of a list control and a thumbnail panel</description>
			<type>facadeview</type>
			<id>50</id>
			<control>
				<description>listcontrol</description>
				<type>listcontrol</type>
				<id>50</id>
				<onleft>2</onleft>
				<onright>50</onright>
				<scrollOffset>1</scrollOffset>
				<posX>524</posX>
				<posY>187</posY>
				<height>458</height>
				<spinPosX>560</spinPosX>
				<spinPosY>688</spinPosY>
				<spinAlign>left</spinAlign>
				<textXOff>22</textXOff>
				<dimColor>ffffffff</dimColor>
			</control>

            <control>
                <description>album view listcontrol</description>
                <type>listcontrol</type>
                <subtype>album</subtype>
                <id>50</id>
				<posX>524</posX>
				<posY>187</posY>
				<height>458</height>
                <textXOff>34</textXOff>
				<textYOff>20</textYOff>
				<textXOff2>710</textXOff2>
				<textYOff2>55</textYOff2>
				<textcolor>ffffffff</textcolor>
				<textcolor2>ff6aa5cb</textcolor2>
                <itemWidth>80</itemWidth>
                <itemHeight>80</itemHeight>
                <IconXOff>21</IconXOff>
                <IconYOff>10</IconYOff>
                <PinIconWidth>20</PinIconWidth>
                <PinIconHeight>20</PinIconHeight>
                <PinIconXOff>72</PinIconXOff>
                <PinIconYOff>22</PinIconYOff>
                <spaceBetweenItems>0</spaceBetweenItems>
                <textureHeight>102</textureHeight>
                <scrollOffset>1</scrollOffset>
				<spinPosX>560</spinPosX>
				<spinPosY>688</spinPosY>
				<spinAlign>left</spinAlign>
				<spinCanFocus>no</spinCanFocus>
				<textureFocus>listcontrol_item_selected_big.png</textureFocus>
				<textureNoFocus>listcontrol_item_big.png</textureNoFocus>
                <onleft>2</onleft>
                <onright>50</onright>
                <keepaspectratio>yes</keepaspectratio>
			</control>
			
			<control>
				<description>Thumbnail Panel</description>
				<type>thumbnailpanel</type>
				<id>50</id>
				<posX>150</posX>
				<posY>240</posY>
				<width>1100</width>
				<height>450</height>
				<spinColor>ffffffff</spinColor>
				<textureUp>page_up_nofocus.png</textureUp>
				<textureDown>page_down_nofocus.png</textureDown>
				<textureUpFocus>page_up_focus.png</textureUpFocus>
				<textureDownFocus>page_down_focus.png</textureDownFocus>
				<scrollOffset>0</scrollOffset>
				<onleft>2</onleft>
				<onright>50</onright>
				<itemWidth>107</itemWidth>
				<itemHeight>107</itemHeight>
				<textureWidth>107</textureWidth>
				<textureHeight>107</textureHeight>
				<thumbWidth>107</thumbWidth>
				<thumbHeight>107</thumbHeight>
				<thumbPosX>1</thumbPosX>
				<thumbPosY>1</thumbPosY>
				<thumbZoom>no</thumbZoom>
				<imageFolder>-</imageFolder>
    			<imageFolderFocus>-</imageFolderFocus>
				<suffix>|</suffix>
				<itemWidthBig>214</itemWidthBig>
				<itemHeightBig>214</itemHeightBig>
				<textureWidthBig>214</textureWidthBig>
				<textureHeightBig>214</textureHeightBig>
				<thumbWidthBig>214</thumbWidthBig>
				<thumbHeightBig>214</thumbHeightBig>
				<thumbPosXBig>1</thumbPosXBig>
				<thumbPosYBig>1</thumbPosYBig>
				<thumbZoom>no</thumbZoom>
				<enableFocusZoom>no</enableFocusZoom>
				<zoomXPixels>50</zoomXPixels>
				<zoomYPixels>50</zoomYPixels>
				<keepaspectratio>no</keepaspectratio>
				<hideUnfocusTexture>no</hideUnfocusTexture>
				<renderFocusText>no</renderFocusText>
				<renderUnfocusText>no</renderUnfocusText>
				<folderPrefix></folderPrefix>
				<folderSuffix></folderSuffix>
				<textureUp>-</textureUp>
				<textureDown>-</textureDown>
				<textureUpFocus>-</textureUpFocus>
				<textureDownFocus>-</textureDownFocus>
				<spinPosX>1190</spinPosX>
			</control>

			<control>
				<description>Filmstrip view</description>
				<type>filmstrip</type>
				<dimColor>90ffffff</dimColor>
				<id>50</id>
				<scrollOffset>2</scrollOffset>
				<folderPrefix></folderPrefix>
				<folderSuffix></folderSuffix>
				<posX>77</posX>
				<posY>255</posY>
				<width>1300</width>
				<height>720</height>
				<onup>2</onup>
				<onleft>50</onleft>
				<thumbWidth>200</thumbWidth>
				<thumbHeight>200</thumbHeight>
				<thumbPosX>4</thumbPosX>
				<thumbPosY>4</thumbPosY>
				<itemWidth>250</itemWidth>
				<itemHeight>231</itemHeight>
				<textureWidth>210</textureWidth>
				<textureHeight>210</textureHeight>
				<thumbs flipY="true" diffuse="Thumb_Mask.png"></thumbs>
				<InfoImage flipY="true" diffuse="Thumb_Mask.png"></InfoImage>
				<showFrame>yes</showFrame>
				<textYOff>-2000</textYOff>
				<showFolder>no</showFolder>
				<showBackGround>no</showBackGround>
				<showInfoImage>no</showInfoImage>
				<thumbAnimation effect="zoom" start="100,100" end="130,130" time="200">focus</thumbAnimation>
				<thumbAnimation effect="zoom" start="130,130" end="100,100" time="200">unfocus</thumbAnimation>
			</control>

        <control>
          <description>Cover Flow view</description>
          <type>coverflow</type>
          <colordiffuse>90ffffff</colordiffuse>
          <dimColor>90ffffff</dimColor>
          <id>50</id>
          <posX>0</posX>
          <posY>300</posY>
          <width>1366</width>
          <height>768</height>
          <onup>2</onup>
          <onleft>50</onleft>
          <onright>50</onright>	
          <selectedCard>0</selectedCard>
          <cardWidth>300</cardWidth>
          <cardHeight>300</cardHeight>
          <angle>65</angle>
		  <sideShift>310</sideShift>
		  <sideGap>210</sideGap>
		  <sideDepth>280</sideDepth>
          <offsetY>0</offsetY>
          <selectedOffsetY>0</selectedOffsetY>
          <speed>8</speed>
          <showFrame>yes</showFrame>
          <frame>coverflow_glass_overlay_music.png</frame>
          <frameFocus>coverflow_glass_overlay_music.png</frameFocus>
          <frameWidth>300</frameWidth>
          <frameHeight>300</frameHeight>
          <spinSpeed>8</spinSpeed>
          <unfocusedAlpha>FF</unfocusedAlpha>
          <folderPrefix></folderPrefix>
          <folderSuffix></folderSuffix>
          <font1>font13</font1>
          <font2>font11</font2>
          <label1>#selecteditem</label1>
          <label2>#selecteditem2</label2>
          <textColor>FFFFFFFF</textColor>
          <remoteColor>FFFF0000</remoteColor>
          <playedColor>FFA0D0FF</playedColor>
          <downloadColor>FF00FF00</downloadColor>
          <selectedColor>FFFFFFFF</selectedColor>
          <shadowAngle>45</shadowAngle>
          <shadowDistance>0</shadowDistance>
          <shadowColor>FF000000</shadowColor>
          <label1YOff>2405</label1YOff>
          <label2YOff>2430</label2YOff>
          <pageSize>5</pageSize>
          <scrollbarBackground>bar_hor.png</scrollbarBackground>
          <scrollbarLeft>mouse_left_horizontal.png</scrollbarLeft>
          <scrollbarRight>mouse_right_horizontal.png</scrollbarRight>
          <scrollbarYOff>455</scrollbarYOff>
          <showScrollbar>no</showScrollbar>
          <cards flipY="yes" diffuse="Thumb_Mask.png" mask="coverflow_poster_mask_music.png"></cards>
          <subitems>
            <subitem>
              <![CDATA[                
			  <controls>
              <control>
                <description>background</description>
                <type>image</type>
                <posX>0</posX>
                <posY>0</posY>
                <width>300</width>
                <height>300</height>
                <texture flipY="yes" diffuse="Thumb_Mask.png" mask="coverflow_poster_mask_music.png">background.png</texture>
              </control>
			  
              <control>
                <description>Musik Details</description>
                <type>label</type>
                <posX>30</posX>
                <posY>10</posY>
                <width>210</width>
                <label>Music Details</label>
                <font>font13</font>
                <textcolor>FFFFFFFF</textcolor>
              </control>
			  
              <control>
                <description>genre</description>
                <type>label</type>
                <posX>30</posX>
                <posY>40</posY>
                <width>210</width>
                <label>#selecteditem</label>
                <font>font10</font>
                <textcolor>FFFFFFFF</textcolor>
              </control>
              
                </controls>
              ]]>
            </subitem>
          </subitems>
        </control>
	  
			<control>
				<description>playlist listcontrol</description>
				<type>playlistcontrol</type>
				<id>50</id>
				<onleft>2</onleft>
				<onright>50</onright>
				<scrollOffset>1</scrollOffset>
				<posY>183</posY>
				<height>450</height>
				<spinCanFocus>no</spinCanFocus>
				<dimColor>ffffffff</dimColor>
			</control>

		</control>

	</control>

</controls>
</window>
