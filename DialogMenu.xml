<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
    <id>2012</id>
    <defaultcontrol>3</defaultcontrol>
    <allowoverlay>no</allowoverlay>
    <controls>
        <control>
            <description>group element</description>
            <type>group</type>
            <animation effect="Fade" time="250">WindowOpen</animation>
            <animation center="683,384" delay="100" effect="Zoom" end="100,100" start="95,95" time="550" tween="Elastic">WindowOpen</animation>
            <animation effect="Fade" time="200">WindowClose</animation>
            <animation center="683,384" effect="Zoom" end="200,200" start="100,100" time="200">WindowClose</animation>
            <control>
                <id>0</id>
                <type>image</type>
                <posX>409</posX>
                <posY>164</posY>
                <width>548</width>
                <height>440</height>
                <texture>context_background.png</texture>
                <animation effect="Fade" time="300">WindowOpen</animation>
                <animation effect="Fade" time="300">WindowClose</animation>
            </control>
            <control>
                <id>4</id>
                <description>Heading text label</description>
                <type>label</type>
                <posX>469</posX>
                <posY>212</posY>
                <font>Segoe Light18</font>
                <textcolor>ff85cffe</textcolor>
                <label>181</label>
                <animation effect="Fade" time="300">WindowOpen</animation>
                <animation effect="Fade" time="300">WindowClose</animation>
            </control>
            <control>
                <id>3</id>
                <description>options listcontrol</description>
                <type>listcontrol</type>
                <onleft>2</onleft>
                <onright>2</onright>
                <posX>442</posX>
                <posY>277</posY>
                <width>482</width>
                <height>300</height>
                <animation effect="Fade" time="300">WindowOpen</animation>
                <animation effect="Fade" time="300">WindowClose</animation>
                <textXOff2>460</textXOff2>
                <textYOff2>10</textYOff2>
                <spinPosX>902</spinPosX>
                <spinPosY>534</spinPosY>
                <textXOff>15</textXOff>
                <textYOff>8</textYOff>
                <textureFocus>context_item_selected.png</textureFocus>
                <textureNoFocus>context_item.png</textureNoFocus>
                <textureHeight>49</textureHeight>
                <spaceBetweenItems>1</spaceBetweenItems>
                <PinIconXOff>2</PinIconXOff>
                <PinIconYOff>11</PinIconYOff>
            </control>
        </control>
    </controls>
</window>
