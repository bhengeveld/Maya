		<font>font12</font>
		<align>left</align>
		<textcolor>ffffffff</textcolor>
	</control>
	
	<control>
		<description>similar videos</description>
		<type>thumbnailpanel</type>
		<id>50</id>
		<posX>45</posX>
		<posY>530</posY>
		<width>1300</width>
		<height>250</height>
		<spinWidth>24</spinWidth>
		<spinHeight>24</spinHeight>
		<spinPosX>60</spinPosX>
		<spinPosY>687</spinPosY>
