		<id>99</id>
		<posX>776</posX>
		<posY>538</posY>
		<width>203</width>
		<height>114</height>
		<action>18</action>
		<onleft>901</onleft>
		<textureFocus>tv_green_border.png</textureFocus>
		<visible>player.hasmedia+!control.isvisible(3337)+[window.istopmost(0)|window.istopmost(34)]</visible>
	</control>

