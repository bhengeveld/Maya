          <itemWidthBig>424</itemWidthBig>
          <itemHeightBig>292</itemHeightBig>
          <textureWidthBig>424</textureWidthBig>
          <textureHeightBig>260</textureHeightBig>
          <thumbWidthBig>380</thumbWidthBig>
          <thumbHeightBig>213</thumbHeightBig>
          <thumbPosXBig>19</thumbPosXBig>
          <thumbPosYBig>19</thumbPosYBig>
