		<texture>-</texture>
	</control>

	<control>
		<description>Description bg</description>
		<type>image</type>
		<id>992</id>
		<posX>191</posX>
		<posY>558</posY>
		<width>984</width>
		<height>117</height>
		<texture>osd_bg_description2.png</texture>
