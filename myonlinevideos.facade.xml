<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
<controls>

	<control>
		<description>group element</description>
		<type>group</type>
		<control>
			<animation effect="fade" time="200">WindowOpen</animation>
			<animation effect="zoom" start="50,50" end="100,100" center="683,384" time="200">WindowOpen</animation>
			<animation effect="fade" time="200">WindowClose</animation>
			<animation effect="zoom" start="100,100" end="50,50" center="683,384" time="200">WindowClose</animation>
			<description>composite control consisting of a list control and a thumbnail panel</description>
			<type>facadeview</type>
			<id>50</id>
			<control>
				<description>listcontrol</description>
				<type>listcontrol</type>
				<id>50</id>
				<onleft>2</onleft>
				<onright>2</onright>
				<scrollOffset>1</scrollOffset>
				<posX>555</posX>
				<posY>183</posY>
				<width>720</width>
				<height>450</height>
				<itemWidth>32</itemWidth>
				<itemHeight>32</itemHeight>
				<IconXOff>12</IconXOff>
				<IconYOff>8</IconYOff>
				<folderPrefix></folderPrefix>
				<folderSuffix></folderSuffix>
			</control>

			<control>
				<description>Thumbnail Panel</description>
				<type>thumbnailpanel</type>
				<id>50</id>
				<posX>163</posX>
				<posY>240</posY>
				<width>1040</width>
				<height>500</height>
				<scrollOffset>0</scrollOffset>
				<folderPrefix></folderPrefix>
				<folderSuffix></folderSuffix>
				<onleft>2</onleft>
				<onright>2</onright>
				<itemWidth>103</itemWidth>
				<itemHeight>140</itemHeight>
				<textureWidth>103</textureWidth>
				<textureHeight>140</textureHeight>
				<thumbWidth>103</thumbWidth>
				<thumbHeight>140</thumbHeight>
				<thumbPosX>0</thumbPosX>
				<thumbPosY>0</thumbPosY>
				<itemWidthBig>148</itemWidthBig>
				<itemHeightBig>210</itemHeightBig>
				<thumbWidthBig>148</thumbWidthBig>
				<thumbHeightBig>210</thumbHeightBig>
				<textureWidthBig>148</textureWidthBig>
				<textureHeightBig>210</textureHeightBig>
				<keepaspectratio>yes</keepaspectratio>
				<thumbPosXBig>0</thumbPosXBig>
				<thumbPosYBig>0</thumbPosYBig>
				<zoomXPixels>50</zoomXPixels>
				<zoomYPixels>79</zoomYPixels>
				<hideUnfocusTexture>yes</hideUnfocusTexture>
				<renderFocusText>no</renderFocusText>
				<renderUnfocusText>no</renderUnfocusText>
				<frame>-</frame>
				<frameFocus>-</frameFocus>
				<thumbAnimation effect="zoom" acceleration="-2" start="100,100" reversible="false" end="135,135" center="0,550" time="400">focus</thumbAnimation>
				<thumbAnimation effect="zoom" start="135,135" reversible="false" end="100,100" center="0,550" time="100">unfocus</thumbAnimation>
				<thumbZoom>no</thumbZoom>
			<enableFocusZoom>yes</enableFocusZoom>
			</control>

		</control>

	</control>

</controls>
</window>
