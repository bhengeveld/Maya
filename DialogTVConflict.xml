<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>2017</id>
  <defaultcontrol>11</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <controls>
  
    <control>
      <type>image</type>
      <id>0</id>
      <width>1920</width>
      <height>1080</height>
      <texture>semi_trans_back_general_menu.png</texture>
      <colordiffuse>90ffffff</colordiffuse>
      <animation effect="fade" time="200">windowopen</animation>
      <animation effect="fade" time="400">windowclose</animation>
      <visible>!window.isvisible(602)+!window.isvisible(2005)</visible>
    </control>
	
    <control>
      <id>0</id>
      <type>image</type>
      <posX>574</posX>
      <posY>230</posY>
      <width>770</width>
      <height>618</height>
      <texture>context_background.png</texture>
    </control>
	
    <control>
      <id>4</id>
      <description>Heading text label</description>
      <type>label</type>
      <posX>660</posX>
      <posY>298</posY>
      <width>598</width>
      <font>Segoe Light18</font>
      <textcolor>ff85cffe</textcolor>
    </control>
	
    <control>
      <id>3</id>
      <description>options listcontrol</description>
      <type>listcontrol</type>
      <onleft>3</onleft>
      <onright>3</onright>
      <ondown>11</ondown>
      <posX>528</posX>
      <posY>389</posY>
      <width>770</width>
      <height>421</height>
      <textXOff2>737</textXOff2>
      <textYOff2>11</textYOff2>
      <textYOff3>14</textYOff3>
      <textXOff3>-2000</textXOff3>
      <spinPosX>1267</spinPosX>
      <spinPosY>752</spinPosY>
      <textXOff>118</textXOff>
      <textYOff>11</textYOff>
      <textureFocus>context_item_selected.png</textureFocus>
      <textureNoFocus>context_item.png</textureNoFocus>
      <textureHeight>68</textureHeight>
      <spaceBetweenItems>1</spaceBetweenItems>
      <PinIconWidth>15</PinIconWidth>
      <PinIconHeight>15</PinIconHeight>
      <PinIconXOff>2000</PinIconXOff>
      <PinIconYOff>15</PinIconYOff>
    </control>
	
    <control>
      <description>Skip new Recording</description>
      <type>button</type>
      <id>11</id>
      <posX>702</posX>
      <posY>562</posY>
      <label>1110</label>
      <onup>3</onup>
      <ondown>12</ondown>
    </control>
	
    <control>
      <description>Skip conflicting Recording</description>
      <type>button</type>
      <id>12</id>
      <posX>702</posX>
      <posY>632</posY>
      <label>1111</label>
      <onup>11</onup>
      <ondown>13</ondown>
    </control>
	
    <control>
      <description>Keep Conflict</description>
      <type>button</type>
      <id>13</id>
      <posX>702</posX>
      <posY>703</posY>
      <label>1112</label>
      <onup>12</onup>
      <ondown>14</ondown>
    </control>
	
    <control>
      <description>Skip conflicting Episode</description>
      <type>button</type>
      <id>14</id>
      <posX>702</posX>
      <posY>773</posY>
      <label>1113</label>
      <onup>13</onup>
      <ondown>3</ondown>
    </control>
	
  </controls>
</window>