<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
<controls>

	<control>
		<description>group element</description>
		<type>group</type>
		<animation effect="fade" time="100">WindowOpen</animation>
		<animation effect="fade" time="100">WindowClose</animation>
		<control>
			<description>composite control consisting of a list control and a thumbnail panel</description>
			<type>facadeview</type>
			<id>50</id>
			<control>
				<description>listcontrol</description>
				<type>listcontrol</type>
				<id>50</id>
				<posY>392</posY>
				<height>300</height>
				<onleft>2</onleft>
				<onright>2</onright>
				<scrollOffset>1</scrollOffset>
				<PinIconHeight>15</PinIconHeight>
				<PinIconXOff>48</PinIconXOff>
				<PinIconYOff>18</PinIconYOff>
				<itemWidth>32</itemWidth>
				<itemHeight>32</itemHeight>
				<IconXOff>12</IconXOff>
				<IconYOff>9</IconYOff>
				<textXOff>24</textXOff>
				<textXOff2>0</textXOff2>
				<keepaspectratio>yes</keepaspectratio>
				<spinCanFocus>no</spinCanFocus>
			</control>

			<control>
				<description>album view listcontrol</description>
				<type>listcontrol</type>
				<subtype>album</subtype>
				<id>50</id>
				<posX>530</posX>
				<posY>150</posY>
				<width>750</width>
				<height>450</height>
				<font>font13</font>
				<font2>font13</font2>
				<selectedColor>80000000</selectedColor>
				<textcolor>ffffffff</textcolor>
				<selectedColor2>ffffffff</selectedColor2>
				<textcolor2>ffffffff</textcolor2>
				<textXOff2>746</textXOff2>
				<textXOff3>108</textXOff3>
				<textYOff2>35</textYOff2>
				<itemWidth>50</itemWidth>
				<itemHeight>50</itemHeight>
				<IconXOff>12</IconXOff>
				<IconYOff>12</IconYOff>
				<PinIconWidth>35</PinIconWidth>
				<PinIconHeight>20</PinIconHeight>
				<PinIconXOff>69</PinIconXOff>
				<PinIconYOff>41</PinIconYOff>
				<spaceBetweenItems>5</spaceBetweenItems>
				<textureHeight>72</textureHeight>
				<scrollOffset>1</scrollOffset>
				<textureFocus>album_background_focus.png</textureFocus>
				<textureNoFocus>album_background_nofocus.png</textureNoFocus>
				<onleft>2</onleft>
				<onright>50</onright>
				<onup>2</onup>
				<ondown>2</ondown>
			</control>

			<control>
				<description>Thumbnail Panel</description>
				<type>thumbnailpanel</type>
				<id>50</id>
				<posX>113</posX>
				<posY>226</posY>
				<width>1200</width>
				<height>450</height>
				<scrollOffset>0</scrollOffset>
				<onleft>2</onleft>
				<onright>50</onright>
				<itemWidth>284</itemWidth>
				<itemHeight>181</itemHeight>
				<textureWidth>175</textureWidth>
				<textureHeight>100</textureHeight>
				<thumbWidth>160</thumbWidth>
				<thumbHeight>90</thumbHeight>
				<thumbPosX>5</thumbPosX>
				<thumbPosY>5</thumbPosY>
				<itemWidthBig>284</itemWidthBig>
				<itemHeightBig>181</itemHeightBig>
				<textureWidthBig>290</textureWidthBig>
				<textureHeightBig>187</textureHeightBig>
				<thumbWidthBig>235</thumbWidthBig>
				<thumbHeightBig>132</thumbHeightBig>
				<thumbPosXBig>27</thumbPosXBig>
				<thumbPosYBig>28</thumbPosYBig>
				<zoomXPixels>0</zoomXPixels>
				<zoomYPixels>0</zoomYPixels>
				<thumbZoom>no</thumbZoom>
				<enableFocusZoom>no</enableFocusZoom>
				<keepaspectratio>no</keepaspectratio>
				<hideUnfocusTexture>no</hideUnfocusTexture>
				<renderFocusText>no</renderFocusText>
				<renderUnfocusText>no</renderUnfocusText>
				<imageFolderFocus>-</imageFolderFocus>
				<imageFolder>-</imageFolder>
				<frameNoFocus>blackborders_recordings_thumbnail_nofocus.png</frameNoFocus>
    <frameFocus>blackborders_recordings_thumbnail_focus.png</frameFocus>
			</control>
			
			<control>
				<description>Filmstrip view</description>
				<type>filmstrip</type>
				<dimColor>90ffffff</dimColor>
				<id>50</id>
				<scrollOffset>2</scrollOffset>
				<folderPrefix></folderPrefix>
				<folderSuffix></folderSuffix>
				<posX>66</posX>
				<posY>255</posY>
				<width>1300</width>
				<height>720</height>
				<onup>2</onup>
				<onleft>50</onleft>
				<thumbWidth>200</thumbWidth>
				<thumbHeight>200</thumbHeight>
				<thumbPosX>4</thumbPosX>
				<thumbPosY>4</thumbPosY>
				<itemWidth>250</itemWidth>
				<itemHeight>231</itemHeight>
				<textureWidth>210</textureWidth>
				<textureHeight>210</textureHeight>
				<thumbs></thumbs>
				<InfoImage></InfoImage>
				<showFrame>yes</showFrame>
				<textYOff>-2000</textYOff>
				<showFolder>no</showFolder>
				<showBackGround>no</showBackGround>
				<showInfoImage>no</showInfoImage>
				<frameFocus>blackborders_recordings.png</frameFocus>
				<thumbAnimation effect="zoom" start="100,100" end="130,130" time="200">focus</thumbAnimation>
				<thumbAnimation effect="zoom" start="130,130" end="100,100" time="200">unfocus</thumbAnimation>
			</control>

		</control>

	</control>

</controls>
</window>
