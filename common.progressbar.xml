<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
<controls>
	<control>
		<type>image</type>
		<description>Dummy image</description>
		<id>105</id>
		<posX>-2000</posX>
		<texture>osd_bg_top.png</texture>
	</control>
	<control>
		<type>image</type>
		<description>Progressbar Background</description>
		<id>994</id>
		<posX>188</posX>
		<posY>664</posY>
		<width>985</width>
		<height>47</height>
		<texture>osd_bg.png</texture>
		<visible>Control.IsVisible(10) | Control.IsVisible(11) | Control.IsVisible(12) | [window.isvisible(2005) + [window.isosdvisible|control.isvisible(105)]]</visible>
	</control>

	<control>
		<type>image</type>
		<description>OSD Pause Background</description>
		<id>993</id>
		<posX>177</posX>
		<posY>653</posY>
		<width>1009</width>
		<height>70</height>
		<texture>osd_bg_pause.png</texture>
 		<visible>Control.IsVisible(10) | Control.IsVisible(11) | Control.IsVisible(12) | [window.isvisible(2005) + [window.isosdvisible|control.isvisible(105)]] + [player.rewinding|player.forwarding|player.paused|player.seeking]]</visible>
	</control>

	<control>
		<type>progress</type>
		<description>Track Progress</description>
		<id>996</id>
		<posX>331</posX>
		<posY>681</posY>
		<width>694</width>
		<height>10</height>
		<label>#percentage</label>
		<texturebg>-</texturebg>
		<lefttexture>osd_progress_left.png</lefttexture>
		<midtexture>osd_progress_mid.png</midtexture>
		<righttexture>osd_progress_right.png</righttexture>
		<visible>window.isosdvisible|player.paused|player.rewinding|player.forwarding|Control.IsVisible(105)|Control.IsVisible(10)|player.seeking</visible>
	</control>

	<control>
		<description>Start Time Info</description>
		<type>label</type>
		<id>997</id>
		<posX>235</posX>
		<posY>675</posY>
		<label>#currentplaytime</label>
		<align>left</align>
		<font>font12</font>
		<visible>window.isosdvisible|player.paused|player.rewinding|player.forwarding|Control.IsVisible(105)|Control.IsVisible(10)|player.seeking</visible>
	</control>

	<control>
		<description>End Time Info</description>
		<type>label</type>
		<id>998</id>
		<posX>1136</posX>
		<posY>675</posY>
		<label>#currentremaining</label>
		<align>right</align>
		<font>font12</font>
		<visible>window.isosdvisible|player.paused|player.rewinding|player.forwarding|Control.IsVisible(105)|Control.IsVisible(10)|player.seeking</visible>
	</control>

	<control>
		<type>label</type>
		<id>999</id>
		<description>Pause</description>
		<posX>575</posX>
		<posY>323</posY>
		<height>30</height>
		<width>144</width>
		<font>font18</font>
		<align>center</align>
		<label>19011</label>
		<visible>!Control.IsVisible(10) + !Control.IsVisible(11) + !Control.IsVisible(12) + [[window.isosdvisible+player.paused]|[!window.isosdvisible+player.paused]]</visible>
	</control>
	
