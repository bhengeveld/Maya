        <posX>2000</posX>
        <posY>2000</posY>
        <width>10</width>
        <height>10</height>
        <texture>-</texture>
        <visible>player.forwarding|player.rewinding|player.paused</visible>
    </control>
	<control>
		<type>image</type>
