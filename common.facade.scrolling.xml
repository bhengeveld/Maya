<window>
  <controls>
    <control>
      <description>listscroller bg</description>
      <type>image</type>
      <id>1</id>
      <posX>683</posX>
      <posY>871</posY>
      <width>96</width>
      <height>146</height>
      <texture>listscroller_bg.png</texture>
