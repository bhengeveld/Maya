<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--version="1.0" encoding="utf-8" standalone="yes"-->
<window>
  <id>800</id>
  <defaultcontrol>2</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <define>#header.label:14001</define>
  <define>#header.background:hover_mpei.png</define>
  <controls>
    <import>common.window.background.xml</import>
    <import>common.time.xml</import>
    <control>
      <id>1</id>
      <type>label</type>
      <description>Header label</description>
      <posX>775</posX>
      <posY>42</posY>
      <width>702</width>
      <height>92</height>
      <font>Segoe Light40</font>
      <textcolor>ff6aa5cb</textcolor>
      <label>#header.label</label>
      <animation effect="Fade" time="100">WindowOpen</animation>
      <animation effect="Fade" time="100">WindowClose</animation>
    </control>
    <control>
      <type>actiongroup</type>
      <description>action menu</description>
      <defaultcontrol>2</defaultcontrol>
      <onexit>50</onexit>
      <buttonX>0</buttonX>
      <buttonY>0</buttonY>
      <buttonwidth>148</buttonwidth>
      <buttonheight>1012</buttonheight>
      <textureFocus>Action_focus.png</textureFocus>
      <textureNoFocus>Action_nofocus.png</textureNoFocus>
      <width>449</width>
      <height>1012</height>
      <dimColor>00ffffff</dimColor>
      <control>
        <type>image</type>
        <posX>36</posX>
        <posY>109</posY>
        <width>515</width>
        <height>831</height>
        <texture>Backslide.png</texture>
        <colorDiffuse>fffffffff</colorDiffuse>
        <visible>!Control.HasFocus(50)</visible>
        <animation effect="slide" time="300" start="-300,0" reversible="false">visible</animation>
        <animation effect="slide" time="300" end="-300,0" reversible="false">hidden</animation>
      </control>
      <control>
        <description>group element</description>
        <type>group</type>
        <animation effect="slide" time="300" start="-300,0" reversible="false">visible</animation>
        <animation effect="slide" time="300" end="-300,0" reversible="false">hidden</animation>
        <posX>56</posX>
        <posY>136</posY>
        <layout>StackLayout</layout>
        <visible>!Control.HasFocus(50)</visible>
        <control>
          <description>View-As</description>
          <type>button</type>
          <id>2</id>
          <label>100</label>
          <onright>50</onright>
          <onup>17</onup>
        </control>
        <control>
          <description>Sort</description>
          <type>sortbutton</type>
          <id>3</id>
          <onright>50</onright>
        </control>
        <control>
          <description>Views</description>
          <type>button</type>
          <id>6</id>
          <label>14002</label>
          <onright>50</onright>
        </control>
      </control>
    </control>
    <import>common.facade.music.xml</import>
  </controls>
</window>