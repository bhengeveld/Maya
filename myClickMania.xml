<window>
  <id>5555</id>
  <defaultcontrol>2</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <define>#header.label:ClickMania</define>
  <define>#header.background:hover_clickmania.png</define>
  <controls>
    <import>common.window.background.xml</import>
    <import>common.time.xml</import>
    <control>
      <id>1</id>
      <type>label</type>
      <description>Header label</description>
      <posX>775</posX>
      <posY>42</posY>
      <width>702</width>
      <height>92</height>
      <font>Segoe Light40</font>
      <textcolor>ff6aa5cb</textcolor>
      <label>#header.label</label>
      <animation effect="Fade" time="100">WindowOpen</animation>
      <animation effect="Fade" time="100">WindowClose</animation>
    </control>
    <control>
      <id>1</id>
      <type>label</type>
      <posX>775</posX>
      <posY>42</posY>
      <width>702</width>
      <height>92</height>
      <font>Segoe Light40</font>
      <textcolor>ff6aa5cb</textcolor>
      <label>#header.label</label>
      <animation effect="Fade" time="100">WindowOpen</animation>
      <animation effect="Fade" time="100">WindowClose</animation>
    </control>
    <control>
      <type>group</type>
      <visible>![control.hasfocus(2)|control.hasfocus(8)|control.hasfocus(4)|control.hasfocus(6)]</visible>
      <dimColor>ffffffff</dimColor>
      <animation effect="Fade" time="250">VisibleChange</animation>
      <animation effect="Fade" time="250">WindowOpen</animation>
      <animation effect="Fade" time="250">WindowClose</animation>
      <control>
        <id>201</id>
        <description>Score: label</description>
        <type>label</type>
        <posX>98</posX>
        <posY>414</posY>
        <label>19005</label>
      </control>
      <control>
        <id>202</id>
        <description>Score</description>
        <type>label</type>
        <posX>407</posX>
        <posY>414</posY>
        <align>RIGHT</align>
        <label>#cm_score</label>
      </control>
      <control>
        <id>203</id>
        <description>Highscore: label</description>
        <type>label</type>
        <posX>98</posX>
        <posY>534</posY>
        <label>19008</label>
      </control>
      <control>
        <id>204</id>
        <description>Highscore</description>
        <type>label</type>
        <posX>407</posX>
        <posY>534</posY>
        <align>RIGHT</align>
        <label>#cm_highscore</label>
      </control>
    </control>
    <control>
      <type>group</type>
      <dimColor>ffffffff</dimColor>
      <animation effect="Fade" time="250">WindowOpen</animation>
