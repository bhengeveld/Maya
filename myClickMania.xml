        <onup>8</onup>
        <ondown>6</ondown>
        <label>19004</label>
        <textalign>RIGHT</textalign>
      </control>
      <control>
        <id>6</id>
        <description>Keyboard</description>
        <type>togglebutton</type>
        <onright>10</onright>
        <onup>4</onup>
        <ondown>2</ondown>
        <label>Keyboard</label>
        <textalign>RIGHT</textalign>
      </control>
    </control>
