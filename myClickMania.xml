      <control>
        <id>10</id>
        <description>ClickMania control</description>
        <type>clickmania</type>
        <onleft>2</onleft>
        <onright>2</onright>
        <onup>2</onup>
        <ondown>2</ondown>
        <colordiffuse>60ffffff</colordiffuse>
        <posX>872</posX>
        <posY>168</posY>
        <width>742</width>
        <height>843</height>
        <nextblocky>100</nextblocky>
        <texturefocus>panel_focus.png</texturefocus>
        <texture>panel_focus.png</texture>
        <font>font18</font>
        <textcolor>ff000000</textcolor>
        <onback>2</onback>
      </control>
    </control>
    <control>
      <description>button group</description>
      <type>group</type>
      <posX>99</posX>
      <posY>170</posY>
      <layout>StackLayout</layout>
      <animation effect="Fade" time="250">WindowOpen</animation>
      <animation effect="Fade" time="250">WindowClose</animation>
      <control>
        <id>2</id>
        <description>New Game</description>
        <type>button</type>
        <onright>10</onright>
        <onup>17</onup>
        <ondown>8</ondown>
        <label>19002</label>
        <textalign>RIGHT</textalign>
      </control>
      <control>
        <id>8</id>
        <description>Take back last move</description>
        <type>button</type>
        <onright>10</onright>
        <onup>2</onup>
        <ondown>4</ondown>
