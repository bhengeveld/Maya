            <scrollbarRight>-</scrollbarRight>
            <scrollbarWidth>689</scrollbarWidth>
			<scrollbarHeight>4</scrollbarHeight>
            <scrollbarYOff>538</scrollbarYOff>
            <spinPosX>-2000</spinPosX>
			<thumbAnimation effect="zoom" acceleration="-2" start="100,100" reversible="false" end="120,120" center="0,700" time="200">focus</thumbAnimation>
			<thumbAnimation effect="zoom" start="120,120" reversible="false" end="100,100" center="0,700" time="100">unfocus</thumbAnimation>
		</control>
		
      <control>
        <description>Thumbnail Panel</description>
