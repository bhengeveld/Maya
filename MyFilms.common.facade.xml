<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
 
 	<!--            :: List Scrolling Popup ::            -->
    
    <control>
        <description>listscroller bg</description>
        <type>image</type>
        <id>1</id>
        <posX>980</posX>
        <posY>320</posY>
        <width>187</width>
        <height>202</height>
        <texture>listscroller_bg.png</texture>
        <visible>facadeview.list+string.contains(#scrolling.up,yes)|string.contains(#scrolling.down,yes)</visible>
        <animation effect="fade" start="0" end="100" time="300" delay="0">Visible</animation>
        <animation effect="fade" start="100" end="0" time="0" delay="0">Hidden</animation>
    </control>
    
    <control>
        <type>label</type>
        <id>1</id>
        <posX>1035</posX>
        <posY>374</posY>
        <width>96</width>
        <height>96</height>
        <font>Segoe Light32</font>
        <textcolor>ff6aa5cb</textcolor>
        <label>#selecteditem.scrolllabel</label>
        <visible>facadeview.list+string.contains(#scrolling.up,yes)|string.contains(#scrolling.down,yes)</visible>
        <align>center</align>
        <valign>middle</valign>
        <animation effect="fade" start="0" end="100" time="300" delay="0">Visible</animation>
        <animation effect="fade" start="100" end="0" time="0" delay="0">Hidden</animation>
    </control>
	
    <control>
      <type>group</type>
      <description>group element</description>
      <control>
        <description>composite control consisting of a list control and a thumbnail panel</description>
        <animation effect="fade" time="100">WindowOpen</animation>
        <animation effect="fade" time="100">WindowClose</animation>
        <type>facadeview</type>
        <id>50</id>
        <control>
          <type>listcontrol</type>
          <description>listcontrol</description>
          <onleft>4</onleft>
          <posX>736</posX>
          <posY>262</posY>
          <height>644</height>
          <width>503</width>
          <spinPosX>787</spinPosX>
          <spinPosY>967</spinPosY>
          <spinAlign>left</spinAlign>
          <textXOff>30</textXOff>
