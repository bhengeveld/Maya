			<folderSuffix></folderSuffix>
			<posX>129</posX>
			<posY>470</posY>
			<width>1920</width>
			<height>768</height>
			<onup>2</onup>
			<ondown>2</ondown>
			<onleft>50</onleft>
			<onright>50</onright>
			<thumbWidth>320</thumbWidth>
			<thumbHeight>450</thumbHeight>
			<thumbPosX>0</thumbPosX>
			<thumbPosY>0</thumbPosY>
			<itemWidth>335</itemWidth>
			<itemHeight>450</itemHeight>
			<textureWidth>320</textureWidth>
			<textureHeight>450</textureHeight>
			<textYOff>-2000</textYOff>
			<imageFolderFocus>-</imageFolderFocus>
			<frame>-</frame>
			<frameFocus>-</frameFocus>
			<background>-</background>
			<thumbs flipY="true" diffuse="Thumb_Mask.png" mask="video_filmstrip_mask.png" />
			<showFrame>yes</showFrame>
			<showFolder>no</showFolder>
			<showBackGround>no</showBackGround>
			<showInfoImage>no</showInfoImage>
            <scrollbarBackground>-</scrollbarBackground>
