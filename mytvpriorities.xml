<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--version="1.0" encoding="utf-8" standalone="yes"-->
<window>
  <id>607</id>
  <defaultcontrol>10</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <define>#header.label:877</define>
  <define>#header.background:hover_my tv.png</define>
  <controls>
    <import>common.window.background.xml</import>
    <import>common.time.xml</import>
    <control>
      <id>1</id>
      <type>label</type>
      <description>Header label</description>
      <posX>775</posX>
      <posY>42</posY>
      <width>702</width>
      <height>92</height>
      <font>Segoe Light40</font>
      <textcolor>ff6aa5cb</textcolor>
      <label>#header.label</label>
      <animation effect="Fade" time="100">WindowOpen</animation>
      <animation effect="Fade" time="100">WindowClose</animation>
    </control>
    <control>
      <type>group</type>
      <description>group element</description>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <control>
        <description>scheduler recordings listcontrol</description>
        <type>updownlistcontrol</type>
        <id>10</id>
        <height>649</height>
        <textXOff>148</textXOff>
        <textYOff>2</textYOff>
        <textXOff2>1048</textXOff2>
        <textYOff2>50</textYOff2>
        <textXOff3>248</textXOff3>
        <textYOff3>50</textYOff3>
        <IconXOff>29</IconXOff>
        <PinIconXOff>50</PinIconXOff>
        <PinIconYOff>15</PinIconYOff>
        <spinPosX>1513</spinPosX>
        <spinPosY>734</spinPosY>
        <itemWidth>87</itemWidth>
        <itemHeight>60</itemHeight>
        <textureFocus>album_background_focus.png</textureFocus>
        <textureNoFocus>album_background_nofocus.png</textureNoFocus>
        <spaceBetweenItems>11</spaceBetweenItems>
        <textureHeight>87</textureHeight>
        <keepaspectratio>no</keepaspectratio>
        <font2>font10</font2>
        <font3>font10</font3>
        <onleft>2</onleft>
        <onright>2</onright>
        <onup>2</onup>
        <ondown>2</ondown>
      </control>
    </control>
    <control>
      <description>Current program title</description>
      <type>fadelabel</type>
      <id>13</id>
      <posX>649</posX>
      <posY>773</posY>
      <width>624</width>
      <label>#TV.Scheduled.Title</label>
      <font>font13</font>
      <textcolor>FFFFFFFF</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <description>Current program time</description>
      <type>label</type>
      <id>14</id>
      <posX>649</posX>
      <posY>808</posY>
      <label>#TV.Scheduled.Time</label>
      <align>left</align>
      <font>font13</font>
      <textcolor>FFB2D4F5</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <description>Description1</description>
      <type>textboxscrollup</type>
      <id>15</id>
      <posX>649</posX>
      <posY>852</posY>
      <width>1073</width>
      <height>113</height>
      <label>#TV.Scheduled.Description</label>
      <font>font13</font>
      <textcolor>FFFFFFFF</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <description>genre</description>
      <type>label</type>
      <id>17</id>
      <posX>1648</posX>
      <posY>808</posY>
      <label>#TV.Scheduled.Genre</label>
      <align>right</align>
      <font>font13</font>
      <textcolor>FFB2D4F5</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
  </controls>
</window>