<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>54537689</id>
  <defaultcontrol>2</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <define>#header.label:Browse the Web</define>
  <define>#header.background:hover_BrowseTheWeb.png</define>
  <disabletopbar>true</disabletopbar>
  <controls>
  
    <import>common.window.background.xml</import>
    <import>common.time.xml</import>
	
    <control>
      <id>1</id>
      <type>label</type>
      <description>Header label</description>
      <posX>185</posX>
      <posY>28</posY>
      <width>500</width>
      <label>#header.label</label>	  
      <font>Segoe Light22</font>
      <textcolor>ff6aa5cb</textcolor>
      <animation effect="Fade" time="200">WindowOpen</animation>
      <animation effect="Fade" time="200">WindowClose</animation>
    </control>

	<control>
		<description>Placeholder for browser component. If statusvisible=true, then bottom coordinate is decreased by the height of control with id of 545376891</description>
		<type>image</type>
		<id>545376890</id>
		<width>1920</width>
		<height>1080</height>
	</control>
		
    <control>
      <description>Status label</description>
      <id>9</id>
      <type>label</type>
      <posX>14</posX>
      <posY>1012</posY>
      <width>1686</width>
      <font>Segoe Light16</font>
      <label>#(L(126)) | #btWeb.status</label>
      <animation effect="fade" time="200">WindowOpen</animation>
      <animation effect="fade" time="200">WindowClose</animation>
      <valign>middle</valign>
	  <visible>string.equals(#btWeb.statusvisible, true)</visible>	  
    </control>
	
    <control>
      <description>Link id label</description>
      <type>label</type>
      <id>9</id>
      <posX>1771</posX>
      <posY>1012</posY>
      <label>#btWeb.linkid</label>
      <font>Segoe Light16</font>
      <align>right</align>
      <textcolor>ffffffff</textcolor>
      <animation effect="fade" time="200">WindowOpen</animation>
      <animation effect="fade" time="200">WindowClose</animation>
      <valign>middle</valign>
	  <visible>string.equals(#btWeb.statusvisible, true)</visible>	  
    </control>
	
  </controls>
</window>