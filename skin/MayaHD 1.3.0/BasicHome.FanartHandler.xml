<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
  
    <!--            :: Series ::            -->
    <control>
      <description>FanartHandler Series 1</description>
      <type>image</type>
      <id>1101</id>
      <posX>0</posX>
      <posY>0</posY>
      <width>1920</width>
      <height>1080</height>
      <visible>Control.IsVisible(1201) + string.equals(#bgimage1,series) + !control.hasfocus(2)+!control.hasfocus(3)+!control.hasfocus(4)+!control.hasfocus(5)</visible>
      <animation effect="fade" start="10" end="100" time="600" reversible="false">Visible</animation>
      <animation effect="fade" start="100" end="10" time="600" reversible="false">Hidden</animation>
      <animation effect="fade" start="100" end="0" time="400" delay="200">WindowClose</animation>
      <animation effect="fade" start="0" end="100" time="400" delay="200">WindowOpen</animation>
      <texture>#fanarthandler.tvseries.backdrop1.any</texture>
    </control>
	
