<?xml version="1.0" encoding="utf-8"?>
<!--
This file should contain the facade view
-->
<window>
<controls>

	<control>
		<type>facadeview</type>
		<id>50</id>
		<control>
			<visible>Control.IsVisible(50)</visible>
			<description>Movie List</description>
			<type>listcontrol</type>
			<id>50</id>
			<onleft>2</onleft>
			<onright>2</onright>
			<posX>131</posX>
			<posY>195</posY>
			<height>450</height>
			<width>270</width>
			<spinPosX>140</spinPosX>
			<spinPosY>690</spinPosY>
			<spinAlign>left</spinAlign>
			<textureFocus>listcontrol_item_selected_small.png</textureFocus>
			<textureNoFocus>listcontrol_item_small.png</textureNoFocus>
			<spinCanFocus>no</spinCanFocus>
		</control>

		<control>
			<description>Filmstrip view</description>
			<type>filmstrip</type>
			<dimColor>90ffffff</dimColor>
			<id>50</id>
				<scrollOffset>2</scrollOffset>
				<folderPrefix></folderPrefix>
				<folderSuffix></folderSuffix>
				<posX>150</posX>
				<posY>355</posY>
				<width>1166</width>
				<height>768</height>
				<onup>2</onup>
				<onleft>2</onleft>
				<thumbWidth>213</thumbWidth>
				<thumbHeight>302</thumbHeight>
				<thumbPosX>-7</thumbPosX>
				<thumbPosY>0</thumbPosY>
				<itemWidth>213</itemWidth>
				<itemHeight>302</itemHeight>
				<textureWidth>213</textureWidth>
				<textureHeight>302</textureHeight>
				<textYOff>-2000</textYOff>
				<imageFolderFocus>-</imageFolderFocus>
				<frame>-</frame>
				<frameFocus>thumbnail_frame_shine.png</frameFocus>
				<background>-</background>
				<thumbs flipY="true" diffuse="Thumb_Mask.png"></thumbs>
				<showFrame>yes</showFrame>
				<showFolder>no</showFolder>
				<showBackGround>no</showBackGround>
				<showInfoImage>no</showInfoImage>
				<thumbAnimation effect="zoom" acceleration="-2" start="100,100" reversible="false" end="138,138" center="0,570" time="400">focus</thumbAnimation>
				<thumbAnimation effect="zoom" start="138,138" reversible="false" end="100,100" center="0,570" time="100">unfocus</thumbAnimation>
		</control>

			<control>
				<description>Thumbnail Panel</description>
				<type>thumbnailpanel</type>
				<id>50</id>
				<posX>163</posX>
				<posY>242</posY>
				<width>1040</width>
				<height>500</height>
				<scrollOffset>0</scrollOffset>
				<folderPrefix></folderPrefix>
				<folderSuffix></folderSuffix>
				<onleft>2</onleft>
				<onright>2</onright>
				<itemWidth>103</itemWidth>
				<itemHeight>140</itemHeight>
				<textureWidth>103</textureWidth>
				<textureHeight>140</textureHeight>
				<thumbWidth>103</thumbWidth>
				<thumbHeight>140</thumbHeight>
				<thumbPosX>0</thumbPosX>
				<thumbPosY>0</thumbPosY>
				<itemWidthBig>148</itemWidthBig>
				<itemHeightBig>210</itemHeightBig>
				<thumbWidthBig>148</thumbWidthBig>
				<thumbHeightBig>210</thumbHeightBig>
				<textureWidthBig>148</textureWidthBig>
				<textureHeightBig>210</textureHeightBig>
				<keepaspectratio>no</keepaspectratio>
				<thumbPosXBig>0</thumbPosXBig>
				<thumbPosYBig>0</thumbPosYBig>
				<zoomXPixels>50</zoomXPixels>
				<zoomYPixels>79</zoomYPixels>
				<hideUnfocusTexture>yes</hideUnfocusTexture>
				<renderFocusText>no</renderFocusText>
				<renderUnfocusText>no</renderUnfocusText>
				<frame>-</frame>
				<frameFocus>thumbnail_frame_shine.png</frameFocus>
				<thumbAnimation effect="zoom" acceleration="-2" start="100,100" reversible="false" end="135,135" center="0,550" time="400">focus</thumbAnimation>
				<thumbAnimation effect="zoom" start="135,135" reversible="false" end="100,100" center="0,550" time="100">unfocus</thumbAnimation>
				<thumbZoom>no</thumbZoom>
			<enableFocusZoom>yes</enableFocusZoom>
			</control>

	</control>

</controls>
</window>
